thislist = ["apple", "banana", "cherry"]
print(thislist)
thislist[1]="mango"

print(thislist)

thislist[1:3]=["blueberry","strawberry"]

print(thislist)

thislist[1:2]=["blueberry","strawberry"]

print(thislist)

thislist[1:]=["blueberry","strawberry"]


print(thislist)

thislist.insert(0,"firstvalue")

# thislist.extend(thislist)

print(thislist)

thislist.remove("apple")

print(thislist)

thislist.pop(1)

print(thislist)

thislist.extend(thislist)

print(thislist)
del thislist[2]

print(thislist)